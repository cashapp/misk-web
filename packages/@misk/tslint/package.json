{
  "name": "@misk/tslint",
  "version": "0.2.0-17",
  "license": "SEE LICENSE IN https://github.com/cashapp/misk-web",
  "description": "Misk-Web TsLint",
  "author": "CashApp/Misk-Web Authors (https://github.com/cashapp/misk-web/graphs/contributors)",
  "main": "lib/web/@misk/tslint/tslint.base.json",
  "repository": {
    "type": "git",
    "url": "git@github.com:cashapp/misk-web.git"
  },
  "scripts": {
    "build": "mkdir -p lib/web/@misk/tslint && cp package.json README.md tslint.base.json lib/web/@misk/tslint",
    "clean": "rm -rf lib package-lock.json yarn.lock",
    "ci-build": "npm run-script clean && npm run-script build",
    "prepare": "npm run-script ci-build"
  },
  "dependencies": {
    "tslint": "6.1.3",
    "tslint-blueprint": "0.1.0",
    "tslint-clean-code": "0.2.10",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "1.16.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.1.0",
    "typescript": "4.1.3"
  }
}
