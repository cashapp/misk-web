{
  "name": "@misk/simpleredux",
  "version": "0.1.27-89",
  "license": "SEE LICENSE IN https://github.com/cashapp/misk-web",
  "description": "Misk-Web SimpleRedux",
  "author": "CashApp/Misk-Web Authors (https://github.com/cashapp/misk-web/graphs/contributors)",
  "main": "lib/web/@misk/simpleredux/simpleredux.js",
  "module": "lib/web/@misk/simpleredux/simpleredux.js",
  "typings": "lib/web/@misk/simpleredux/src/index.d.ts",
  "engines": {
    "node": ">=7.8.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cashapp/misk-web.git"
  },
  "scripts": {
    "build": "npm run-script lint && npm run-script lib && npm run test",
    "clean": "rm -rf lib",
    "dev": "npm run-script build; npm run-script updateDevCache",
    "ci-build": "npm run-script clean && npm run-script lib && npm run test",
    "lib": "webpack",
    "lint": "prettier --write --config package.json \".{/src/**/,test/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "gradle": "cd ../../../../../ && ./gradlew web -Pcmd='-ca' -Ptabs='@misk/simpleredux'",
    "prepare": "npm run-script ci-build",
    "reinstall": "rm -rf node_modules && npm install",
    "start": "npm run-script lint && cross-env NODE_ENV=development webpack-dev-server",
    "test": "jest --passWithNoTests  --maxWorkers=2",
    "updateDevCache": "cd ..; ./updateLocalCache.sh"
  },
  "dependencies": {
    "@blueprintjs/core": "3.19.1",
    "@blueprintjs/datetime": "3.13.0",
    "@blueprintjs/icons": "3.13.0",
    "@blueprintjs/select": "3.15.0",
    "@blueprintjs/table": "3.8.18",
    "@blueprintjs/timezone": "3.8.0",
    "@misk/common": "0.1.27-89",
    "@redux-saga/core": "1.1.3",
    "axios": "0.21.1",
    "connected-react-router": "6.8.0",
    "dayjs": "1.10.3",
    "enum-utils": "1.0.0",
    "history": "4.10.1",
    "http-method-enum": "1.0.0",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.20",
    "re-reselect": "4.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-hot-loader": "4.13.0",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-transition-group": "4.4.1",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "skeleton-css": "2.0.4",
    "status-code-enum": "1.0.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@misk/dev": "0.1.27-89",
    "@misk/prettier": "0.1.27-89",
    "@misk/test": "0.1.27-89",
    "@misk/tslint": "0.1.27-89",
    "@testing-library/dom": "7.22.2",
    "@testing-library/react": "10.4.8",
    "@types/history": "4.7.8",
    "@types/jest": "26.0.14",
    "@types/lodash": "4.14.167",
    "@types/node": "11.13.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "7.1.15",
    "@types/react-router": "5.1.11",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.7",
    "@types/webpack": "4.41.26",
    "@types/webpack-env": "1.16.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.3",
    "css-loader": "5.0.1",
    "fibers": "5.0.0",
    "file-loader": "6.2.0",
    "html-webpack-harddisk-plugin": "1.0.2",
    "html-webpack-plugin": "4.5.1",
    "jest": "24.9.0",
    "jest-emotion": "10.0.14",
    "jest-serializer-html": "7.0.0",
    "mini-css-extract-plugin": "1.3.3",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "sass": "1.32.2",
    "sass-loader": "10.1.1",
    "source-map-loader": "2.0.0",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.14",
    "tslib": "2.1.0",
    "typescript": "3.5.3",
    "typescript-plugin-styled-components": "1.4.4",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "4.3.0",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.1",
    "webpack-merge": "5.7.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "jest-serializer-html",
      "jest-emotion"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "^tests(.*)$": "<rootDir>/tests/$1"
    }
  },
  "prettier": "@misk/prettier",
  "miskTab": {
    "version": "0.1.1"
  }
}
