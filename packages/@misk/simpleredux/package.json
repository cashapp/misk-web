{
  "name": "@misk/simpleredux",
  "version": "0.1.26-84",
  "license": "SEE LICENSE IN https://github.com/cashapp/misk-web",
  "description": "Misk-Web SimpleRedux",
  "author": "CashApp/Misk-Web Authors (https://github.com/cashapp/misk-web/graphs/contributors)",
  "main": "lib/web/@misk/simpleredux/simpleredux.js",
  "module": "lib/web/@misk/simpleredux/simpleredux.js",
  "typings": "lib/web/@misk/simpleredux/src/index.d.ts",
  "engines": {
    "node": ">=7.8.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cashapp/misk-web.git"
  },
  "scripts": {
    "build": "npm run-script lint && npm run-script lib && npm run test",
    "clean": "rm -rf lib",
    "dev": "npm run-script build; npm run-script updateDevCache",
    "ci-build": "npm run-script clean && npm run-script lib && npm run test",
    "lib": "webpack",
    "lint": "prettier --write --config package.json \".{/src/**/,test/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "gradle": "cd ../../../../../ && ./gradlew web -Pcmd='-ca' -Ptabs='@misk/simpleredux'",
    "prepare": "npm run-script ci-build",
    "reinstall": "rm -rf node_modules && npm install",
    "start": "npm run-script lint && cross-env NODE_ENV=development webpack-dev-server",
    "test": "jest --passWithNoTests  --maxWorkers=2",
    "updateDevCache": "cd ..; ./updateLocalCache.sh"
  },
  "dependencies": {
    "@blueprintjs/core": "3.19.1",
    "@blueprintjs/datetime": "3.13.0",
    "@blueprintjs/icons": "3.13.0",
    "@blueprintjs/select": "3.12.2",
    "@blueprintjs/table": "3.8.4",
    "@blueprintjs/timezone": "3.6.2",
    "@misk/common": "0.1.26-84",
    "@redux-saga/core": "1.1.3",
    "axios": "0.19.2",
    "connected-react-router": "6.6.1",
    "dayjs": "1.8.24",
    "enum-utils": "1.0.0",
    "history": "4.10.1",
    "http-method-enum": "1.0.0",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.15",
    "re-reselect": "3.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-hot-loader": "4.12.20",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "skeleton-css": "2.0.4",
    "status-code-enum": "1.0.0",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@misk/dev": "0.1.26-84",
    "@misk/prettier": "0.1.26-84",
    "@misk/test": "0.1.26-84",
    "@misk/tslint": "0.1.26-84",
    "@testing-library/dom": "7.2.2",
    "@testing-library/react": "9.1.1",
    "@types/history": "4.7.5",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.150",
    "@types/node": "11.13.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-helmet": "5.0.15",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "7.1.7",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.4",
    "@types/styled-components": "4.4.3",
    "@types/webpack": "4.41.11",
    "@types/webpack-env": "1.15.1",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.2",
    "css-loader": "3.5.2",
    "fibers": "4.0.2",
    "file-loader": "6.0.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.14",
    "jest-serializer-html": "7.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.4",
    "ts-jest": "24.3.0",
    "ts-loader": "7.0.1",
    "tslib": "1.11.1",
    "typescript": "3.5.3",
    "typescript-plugin-styled-components": "1.4.4",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "jest-serializer-html",
      "jest-emotion"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "^tests(.*)$": "<rootDir>/tests/$1"
    }
  },
  "prettier": "@misk/prettier",
  "miskTab": {
    "version": "0.1.1"
  }
}
