    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-hot-loader": "4.11.0",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-transition-group": "4.1.1",
    "redux": "4.0.1",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "re-reselect": "3.2.0",
